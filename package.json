  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --progress",
    "deploy": "yarn run build && yarn run surge",
    "surge": "surge -d react-startup-template.surge.sh -p ./public",
    "build": "node reset && webpack --display-error-details --config webpack.config.production.js",
    "preview": "yarn run build -- -p 3001",
    "reset": "node reset",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tag": "gulp scaffold:tag --name",
    "component": "gulp scaffold:component --name",
    "mobx-component": "gulp scaffold:mobx-component --name",
    "form-component": "gulp scaffold:form-component --name",
    "webpack-build": "webpack --config ./build/webpack/webpack.config.js --progress",
    "webpack-watch": "webpack-dev-server --config ./build/webpack/webpack.config.js --progress"
  },
